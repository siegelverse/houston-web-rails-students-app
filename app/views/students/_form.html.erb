<%= form_for @student do |f| %>
    <%= f.label :name %> 
    <%= f.text_field :name %> 
    <%= f.label :major %> 
    <%= f.text_field :major %>
    <%= f.label :age %> 
    <%= f.number_field :age %>
    <%= f.collection_select :instructor_id, Instructor.all, :id, :name %>
    <%= f.submit %>
<%end%>